<%= @result %>
<%= form_for @product, :url => { :action => "create" } do |f| %>
                        <%= f.label :name %>
                        <%= f.text_field :name %>

                        <%= f.label :value%>
                        <%= f.text_field :value%>

                        <%= f.label :quantity%>
                        <%= f.text_field :quantity%>


                        <%= f.submit "Registrar producto" %>
                <% end %>
<%= debug(params) if Rails.env.development? %>
